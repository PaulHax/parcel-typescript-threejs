{
  "name": "parcel-typescript-threejs",
  "version": "1.0.0",
  "description": "Boilerplate Parcel + TypeScript + ThreeJs",
  "scripts": {
    "start": "npm run clean && parcel src/index.html",
    "build": "npm run clean && parcel build src/index.html --public-url ./",
    "clean": "rimraf ./dist ./.cache",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "Parcel",
    "TypeScript",
    "ThreeJs",
    "Three.js"
  ],
  "author": "Adri√°n Pucheta <pucheta.adrian@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "three": "^0.112.0"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "assets",
        "staticOutDir": "assets"
      }
    ]
  }
}
